class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        int n=arr.size(),j=0;
        vector<int> vec;
        for(int i=1;i<=2000;i++)
        {
            if(j<n&&arr[j]==i)
            {
                j++;
            }else {
                vec.push_back(i);
            }
            if ((int)vec.size() == k) return vec.back(); // early stop
        }
        return -1;
        
        
    }
};
