class Solution {
public:
    bool isAnagram(string s, string t) {
        map<char,int> mpp1;
        map<char,int> mpp2;
        int n=s.size();
        int m=t.size();
        if(n!=m)
        return false;

        for(int i=0;i<n;i++)
        {
            mpp1[s[i]]++;
        }
        for(int i=0;i<m;i++)
        {
            mpp2[t[i]]++;
        }
        
        auto it1 = mpp1.begin();
        auto it2 = mpp2.begin();

        while (it1 != mpp1.end() && it2 != mpp2.end()) {
            if((it1->first!=it2->first)||(it1->second!=it2->second))
            {
                return false;
            }
            ++it1;
            ++it2;
        }
        return true;
    }
};
